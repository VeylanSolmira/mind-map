{
  "name": "mind-map-backend",
  "version": "1.0.0",
  "description": "Backend for AI Development Knowledge Management System",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "ts-node scripts/seedFromCSV.ts",
    "migrate": "ts-node scripts/migrateToId.ts",
    "ingest-research": "ts-node scripts/ingestResearchTabs.ts"
  },
  "keywords": [
    "ai",
    "knowledge-management",
    "mind-map",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/csvtojson": "^1.1.5",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.19.103",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^2.0.22",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
